<!DOCTYPE html>
<html>
<head>
  <title>URLockBox</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div class="container">
    <% if current_user %>
    <%= link_to "Logout", logout_path, method: :delete  %>
    <% end %>
    <%= yield %>
  </div>

</body>
</html>
